<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>cvsi-backend</application>
    <version>1</version>
    <threadsafe>true</threadsafe>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>

    <env-variables>
        <env-var name="ENDPOINTS_SERVICE_NAME" value="cvsi-backend.appspot.com"/>
        <env-var name="ENDPOINTS_SERVICE_VERSION" value="2017-10-27r0"/>
    </env-variables>
</appengine-web-app>
